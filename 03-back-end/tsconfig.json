{
    "compilerOptions": {
        "module": "CommonJS",
        "declaration": true,
        "removeComments": true,
        "target": "ES6",
        "sourceMap": true,
        "outDir": "./dist",
        "allowJs": false, //ovo je resilo problem sa override-om iz tsconfig.json
        "typeRoots": [
            "@types",
            "./node_modules/@types",
        ],
    },
    "exclude": [
        "node_modules",
        "dist",
    ]
}